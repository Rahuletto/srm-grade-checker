{
  "compilerOptions": {
    "jsx": "react",
    "target": "ES2022",
    "module": "esnext",
    "lib": ["es6"],
    "allowJs": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "exclude": [
    "node_modules"
  ],
  "extends": "astro/tsconfigs/strict"
}